name: creditscoring-mlflow-project

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      data_path: {type: path, default: creditscoring_preprocessing/creditscoring_preprocessed.csv}
      target_col: {type: str, default: target}
    command: >
      python modelling.py
      --data-path {data_path}
      --target-col {target_col}
